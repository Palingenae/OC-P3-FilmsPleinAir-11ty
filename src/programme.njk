---
title: "Programme"
permalink: "/{{ title | slugify }}.html"
---

{% extends "_layouts/base.njk" %}

{% block title %} - {{ title }}{% endblock %}

{% block body %}
    <main class="main-content">
        <h2>{{ title }}</h2>
        <section class="section content-listing">
        {% for moviesSessions in getAllMovies %}
            <div class="program__day">
                <h3>{{ moviesSessions[0] }}</h3>
                <div class="program__day__list">
                    {% for session in moviesSessions[1] %}
                    <div class="program__movie">
                        <p class="program__movie__time">{{ session.time }}</p>
                        <h4 class="program__movie__title">
                            <a href="/programme/{{ session.slug }}">{{ session.title }}</a>
                        </h4>
                        <p class="program__movie__duration">{{ session.duration }}</p>
                    </div>
                    {% endfor %}
                </div>
            </div>
        {% endfor %}
        </section>
        {% include "_partials/booking_cta.njk" %}
    </main>
{% endblock %}